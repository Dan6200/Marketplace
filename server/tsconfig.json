{
	"compilerOptions": {
		"outDir": "built",
		"allowJs": true,
		"target": "es5",
		"esModuleInterop": true,
		"moduleResolution": "Node",
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"baseUrl": "./src",
		"paths": {
			"*": [
				"*",
				"tests/*",
				"routes/*",
				"controllers/*",
				"errors/*",
				"middleware/*",
				"app-schema/*"
			]
		}
	},
	"include": ["./src/"],
	"exclude": ["node_modules/", "dist/"]
}
